{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Grpc": "Information",
      "Microsoft": "Information"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:80",
        "Protocols": "Http1AndHttp2"
      },
      "HttpsCustomCert": {
        "Url": "https://*:443",
        "Certificate": {
          "Path": "/data/hotswap/cert.pfx"
        }
      }
    }
  }
}
