    "@angular/platform-browser-dynamic": "11.2.14",
    "@angular/router": "11.2.14",
    "@auth0/auth0-angular": "1.6.0",
    "google-protobuf": "3.18.1",
    "grpc-web": "1.2.1",
    "howler": "2.2.3",
    "rxjs": "6.6.7",
    "systemjs": "6.10.3",
    "tslib": "2.3.1",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.1.1",
