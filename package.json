    "jasmine-core": "3.9.0",
    "jasmine-spec-reporter": "6.0.0",
    "karma": "5.2.3",
    "karma-chrome-launcher": "3.1.0",
    "karma-firefox-launcher": "2.1.1",
    "karma-jasmine": "4.0.1",
    "karma-requirejs": "1.1.0",
    "karma-sourcemap-loader": "0.3.8",
    "lint-staged": "10.5.4",
    "prettier": "2.4.1",
    "protobufjs": "6.11.2",
    "protractor": "7.0.0",
    "requirejs": "2.3.6",
    "rollup": "2.58.0",
    "terser": "5.9.0",
    "tslint": "6.1.3",
    "typescript": "4.5.3",
    "umd-compat-loader": "2.1.2",
    "webpack": "5.56.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{tsx,ts,jsx,js,json,scss}": [
      "prettier --write",
      "git add"
    ],
    "*.{cs,csx,csproj}": [
      "bash ./dotnet-format.sh",
      "git add"
    ],
    "(BUILD|WORKSPACE|*.bazel|*.bzl)": [
      "buildifier"
    ],
    "*.proto": [
      "clang-format -i"
    ]
  }
}
