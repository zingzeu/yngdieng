{
  "name": "web2",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc",
    "serve": "ibazel run //web/src:devserver -- -port=4200",
    "admin:serve": "ibazel run //web/admin:devserver -- -port=4200",
    "admin:serve:prod": "ibazel run --config=release //web/admin:prodserver -- --port 4200",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "engines": {
    "node": ">=10.9.0 <13.0.0",
    "yarn": ">=1.9.2 <2.0.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.2.0",
    "@angular/cdk": "10.2.5",
    "@angular/common": "10.2.0",
    "@angular/core": "10.2.0",
    "@angular/forms": "10.2.0",
    "@angular/material": "10.2.5",
    "@angular/platform-browser": "10.2.0",
    "@angular/platform-browser-dynamic": "10.2.0",
    "@angular/router": "10.2.0",
    "@auth0/auth0-angular": "1.2.0",
    "google-protobuf": "3.13.0",
    "grpc-web": "1.2.1",
    "howler": "2.2.0",
    "rxjs": "6.6.3",
    "systemjs": "6.7.1",
    "tslib": "2.0.3",
    "zone.js": "0.11.2"
  },
  "devDependencies": {
    "@angular/cli": "10.2.0",
    "@angular/compiler": "10.2.0",
    "@angular/compiler-cli": "10.2.0",
    "@babel/cli": "7.12.1",
    "@babel/core": "7.12.3",
    "@babel/preset-env": "7.12.1",
    "@bazel/angular": "2.2.2",
    "@bazel/bazelisk": "1.7.3",
    "@bazel/benchmark-runner": "0.1.0",
    "@bazel/buildifier": "3.5.0",
    "@bazel/ibazel": "0.12.4",
    "@bazel/jasmine": "2.2.2",
    "@bazel/karma": "2.2.2",
    "@bazel/labs": "2.2.2",
    "@bazel/protractor": "2.2.2",
    "@bazel/rollup": "2.2.2",
    "@bazel/terser": "2.2.2",
    "@bazel/typescript": "2.2.2",
    "@grpc/proto-loader": "0.5.5",
    "@rollup/plugin-commonjs": "15.1.0",
    "@rollup/plugin-node-resolve": "9.0.0",
    "@types/google-protobuf": "3.7.3",
    "@types/jasmine": "3.6.0",
    "@types/node": "6.14.13",
    "clang-format": "1.4.0",
    "core-js": "2.6.11",
    "history-server": "1.3.1",
    "html-insert-assets": "0.12.0",
    "husky": "4.3.0",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "6.0.0",
    "karma": "5.2.3",
    "karma-chrome-launcher": "3.1.0",
    "karma-firefox-launcher": "2.0.0",
    "karma-jasmine": "4.0.1",
    "karma-requirejs": "1.1.0",
    "karma-sourcemap-loader": "0.3.8",
    "lint-staged": "10.4.2",
    "opencc": "1.1.1",
    "prettier": "2.1.2",
    "protobufjs": "6.10.1",
    "protractor": "7.0.0",
    "requirejs": "2.3.6",
    "rollup": "2.32.1",
    "terser": "5.3.8",
    "tslint": "6.1.3",
    "typescript": "4.0.3",
    "webpack": "5.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{tsx,ts,jsx,js,json,scss}": [
      "prettier --write",
      "git add"
    ],
    "*.{cs,csx,csproj}": [
      "bash ./dotnet-format.sh",
      "git add"
    ],
    "(BUILD|WORKSPACE|*.bazel|*.bzl)": [
      "buildifier"
    ],
    "*.proto": [
      "clang-format -i"
    ]
  }
}
